<%= form_for [@startupp, @review] do |f| %>
 <%= render "shared/errors", object: @review %>
<fieldset>
<p><%= f.label :name %><%= f.text_field :name, autofocus: true %></p>
<p><%= f.label :ratings %> 
<% Review::RATINGS.each do |r| %>
<%= f.radio_button :ratings, r %> <%= r %> <% end %>
 <%#= f.select :ratings, Review::RATINGS, prompt: "Pick one" %></p>  
 <p><%= f.label :comment %>
  <%= f.text_area :comment, cols: 80, placeholder: "Feel free to rate and review this startup" %></p>
 <p><%= f.submit 'Post Review' %> 
    <%= link_to "Cancel", @startupp %>
    </p>
  </fieldset>  
<% end %>